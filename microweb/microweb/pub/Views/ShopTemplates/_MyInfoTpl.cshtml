<script id="RegisterFirstTemplate" type="text/mustache-template">

	<div id="header">
		<div class="head">
			<h1>用户注册</h1>
			<a class="H-back" href="javascript:history.back(-1);">返回</a>
			<a class="H-home" href="/#page/Catalog">首页</a>
		</div>
	</div>

	<div id="jqt" data-role="page" class="wrapper">
		<div data-role="content" class="pt3m PersDate">
			<div class="E-Box mt00">
				<div class="Item">
					<input type="text" id="r_Phone" placeholder="输入手机号" />
					<button class="Getcode" id="GetCode">获取验证码</button>
				</div>
				<div class="Item">
					<input type="text" id="VerCode" placeholder="输入验证码" />
				</div>

			</div>
			<div class="mt3m pdrd">
				<a href="javascript:void(0)" class="orange field_btn" id="registerFirstBtn">下一步</a>
			</div>
		</div>
	</div>
</script>

<script id="RegisterSecondTemplate" type="text/mustache-template">

    <div id="header">
        <div class="head">
            <h1>用户注册</h1>
            <a class="H-back" href="/#page/Register">返回</a>
            <a class="H-home" href="/#page/Catalog">首页</a>
        </div>
    </div>

    <div id="jqt" data-role="page" class="wrapper">
        <div data-role="content" class="pt3m PersDate">
            <div class="E-Box mt00">

                <div class="Item">
                    <input type="password" id="r_password" placeholder="密码" />
                </div>
                <div class="Item">
                    <input type="password" id="r_password_confirm" placeholder="确认密码" />
                </div>
                <div class="Item">
                    <input type="text" id="r_CommendPeople" placeholder="推荐人" autocomplete="off" />
                </div>

            </div>
            <div class="ErrorTips" id="Error"></div>
            <div class="mt3m pdrd">

                <a href="javascript:void(0)" class="orange field_btn" id="registerNextBtn">确认注册</a>
            </div>
        </div>

        <!--反馈提示-->
        <div class="PopTips">
            修改成功
        </div>
    </div>
</script>

