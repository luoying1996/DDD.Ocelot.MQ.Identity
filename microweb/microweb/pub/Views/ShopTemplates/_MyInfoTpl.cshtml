<script id="LoginTemplate" type="text/mustache-template">
    {{>header}}
    <div data-role="content" class="pt3m">
        <div class="mt7m fields">
            <div class="field white"><div class="field_input"><input type="text" id="UserName" placeholder="用户名/手机/邮箱" autocomplete="off" /></div></div>
            <div class="field white"><div class="field_input"><input type="password" id="Password" placeholder="密码" autocomplete="off" /></div></div>
            <div class="ErrorTips" id="loginError"></div>
        </div>
        <div class="pd1m">

            <div class="mt1m pdrd">
                <a href="javascript:void(0)" class="orange field_btn" id="loginBtn">登录并绑定</a>
            </div>
            <div class="fd_tc rememtip">
                还没有账号？<a href="/#page/Register">快速注册</a><a href="/#page/GetLostPassWord">忘记密码</a>
            </div>
        </div>
    </div>
    <div class="navfix">
        <ul class="clearfix">
            <li class="F-home"><a href="/#page/Catalog"><em></em><p>主页</p></a></li>
            <li class="F-cate"><a href="/#page/FirstCategory?0"><em></em><p>类目</p></a></li>
            <li class="F-cart"><a href="/#page/MyCart"><em></em><p>购物车</p></a></li>
            <li class="F-member cur"><a href="/#page/MyUserInfo"><em></em><p>我的</p></a></li>
        </ul>
    </div>



</script>

